  @@map("_fivem_users")
}

model fivem_vehicles {
  vehicleid               Int                       @id @default(autoincrement())
  vehicleuid              String?                   @default(dbgenerated("(uuid())")) @db.VarChar(255)
  ownerid                 Int?
